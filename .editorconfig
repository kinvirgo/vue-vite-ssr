# EditorConfig 是用来抹平编辑器差异的，比如文件编码，锁进格式等
# 参考https://linghuam.github.io/2017/12/28/editorconfig%E9%85%8D%E7%BD%AE%E6%96%87%E4%BB%B6%E8%AF%A6%E8%A7%A3/
root=true

# 对所有文件有效
[*]
# 文件编码utf-8
charset = utf-8
# 文件换行符
end_of_line = crlf
# 空格缩进
indent_style = space
# 缩进空格为2个
indent_size = 4
# 不保留行末的空格
trim_trailing_whitespace = true
#文件末尾添加一个空行
insert_final_newline = true
